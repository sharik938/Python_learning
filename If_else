#this code is to practise if / else
Language = [ "python", "go", "java" ]
Tech = [ "Yes", "yes"]
print( "Lets see if you are eligible for Dev Ops role: ")
Skill1 = input("What coding language you know, Options " + str(Language) + ": ")
Skill2 = input("Do you know K8s?  (yes/no) :")
Skill3 = input("Do you know Azure? ")
Skill4 = input("Number of years you have worked in cloud? :")
if Skill4.isdigit() == False:
    print("Please enter only numbers for years")
elif Skill1.lower() in Language and (Skill2 in Tech or Skill3 in Tech) and int(Skill4) >= 5:
    print("Congrats you have right skill set for this Role!!")
else:
    print("Sorry you lack expertise for this role " + str(Language) )
